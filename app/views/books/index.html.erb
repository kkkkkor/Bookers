<!--投稿一覧画面-->
 <p id="notice"></p>
  <h1>Books</h1>
<table>
  <thead>
    <tr>
        <th>title</th>
        <th>body</th>
    </tr>
   </thead>

  <tbody>
    <% @books.each do |list| %>
    <tr>
    <td><%= list.title %></td>
    <td><%= list.body %></td>
     <td><a class="show_92720" href="/books/92720">Show</a></td>
        <td><a class="edit_92720" href="/books/92720/edit">Edit</a></td>
        <td><a data-confirm="Are you sure?" class="destroy_92720" rel="nofollow" data-method="delete" href="/books/92720">Destroy</a></td>
    </tr>
    <% end %>
  </tbody>
</table>
<h2>New book</h2>
<%= form_with model: @book do |f| %>
<h4>title</h4>
<%= f.text_field :title %>
<h4>body</h4>
<%= f.text_area :body %>

<%= f.submit "create Book"%>
<!--<span>-->
<!-- link_to index.title, "/books/#[books:id]" %>-->
<!--</span>-->
<% end %>